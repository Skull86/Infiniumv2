{
    "root": true,
    "env": {
      "browser": true,
      "es2021": true,
      "node": true,
      "vue/setup-compiler-macros": true
    },
    "extends": [
      "plugin:vue/vue3-recommended",
      "plugin:vue/vue3-strongly-recommended",
      "eslint:recommended",
      "@vue/typescript/recommended",
      "@vue/prettier",
      "@vue/prettier/@typescript-eslint"
    ],
    "parserOptions": {
      "ecmaVersion": 2021
    },
    "plugins": [],
    "rules": {
      "no-console": "warn",
      "@typescript-eslint/ban-ts-comment": "off"
    }
  }